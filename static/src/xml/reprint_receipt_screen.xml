<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-inherit="point_of_sale.ReprintReceiptScreen" t-inherit-mode="extension">
        <xpath expr="//button[hasclass('print')]" position="before">
            <button class="button print btn btn-lg btn-primary mb-1" t-on-click="tryOnlineOrderReprint">
                <i class="fa fa-print ms-2"></i> Print Online Order Receipt
            </button>
        </xpath>
    </t>
</templates>
